class Node:
    def __init__(self, value) -> None:
        self.value = value
        self.left = None
        self.right = None

class BinarySearchTree:
    def __init__(self) -> None:
        self.root = None

    def append(self, value):
        new_node = Node(value)
        if self.root is None:
            self.root = new_node
            return True
        else:
            temp = self.root
            while True:
                if temp.value == new_node.value:
                    return False
                else:
                    if temp.value > new_node.value:
                        if temp.left == None:
                            temp.left = new_node
                            return True
                        temp = temp.left
                    else:
                        if temp.right == None:
                            temp.right = new_node
                            return True
                        temp = temp.right

my_bst = BinarySearchTree()
